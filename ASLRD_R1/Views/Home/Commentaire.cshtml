@model IEnumerable<ASLRD_R1.DataBaseAccess.commentaire>

@{
    ViewBag.Title = "Commentaire";
}

<h2>Commentaire</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.commentaire1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.note)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.datecommentaire)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.restaurantID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.clientID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.commentaire1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.note)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.datecommentaire)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.restaurantID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.clientID)
        </td>       
    </tr>
}

</table>
